{
  "name": "com.mc2.engsql2",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "update-jeep": "npm install --save @capacitor-community/sqlite@latest --save-dev vue-sqlite-hook@latest --save-dev jeep-sqlite@latest",
    "dev": "npm run copysqlwasm && vite",
    "dev-cli": "npm run copysqlwasm && vue-cli-service serve",
    "build": "npm run copysqlwasm && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --fix --ignore-path .gitignore",
    "copysqlwasm": "copyfiles -u 3 node_modules/sql.js/dist/sql-wasm.wasm public/assets"
  },
  "dependencies": {
    "@awesome-cordova-plugins/core": "^6.2.0",
    "@awesome-cordova-plugins/diagnostic": "^6.2.0",
    "@capacitor/android": "^4.5.0",
    "@capacitor/cli": "^4.5.0",
    "@capacitor/core": "^4.5.0",
    "@capacitor/filesystem": "^4.1.4",
    "@capacitor/share": "^4.1.0",

    "cordova.plugins.diagnostic": "^7.1.1",
    "pinia": "^2.0.26",
    "vue": "^3.2.45",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@capacitor-community/sqlite": "^4.5.0",
    "@rushstack/eslint-patch": "^1.1.4",
    "@vitejs/plugin-vue": "^3.2.0",
    "@vue/cli-service": "~5.0.0",
    "@vue/eslint-config-prettier": "^7.0.0",
    "copyfiles": "^2.4.1",
    "eslint": "^8.22.0",
    "eslint-plugin-vue": "^9.3.0",
    "prettier": "^2.7.1",

  }
}
